---
import MainLayout from "../layouts/MainLayout.astro";
import Button from "../components/Button.astro";
import Link from "../components/Link.astro";
import { Image } from "astro:assets";
import thumbsup from "../assets/images/thumbsup.png";
import forside from "../assets/images/forside.webp";
import kaffer from "../assets/images/kaffer.webp";
import LysBorderButton from "../components/LysBorderButton.astro";
import LysButton from "../components/LysButton.astro";
import kaffeflyver from "../assets/images/kaffeflyver.webp";
import steps from "../assets/images/steps.png";
import ansatte from "../assets/images/ansatte.webp";
import farm from "../assets/images/farm.webp";
import farmer2 from "../assets/images/farmer2.webp";
import farmer3 from "../assets/images/farmer3.webp";
import farmer4 from "../assets/images/farmer4.webp";
import GronBorderButton from "../components/GronBorderButton.astro";
import GronButton from "../components/GronButton.astro";
import RodBorderButton from "../components/RodBorderButton.astro";
import RodButton from "../components/RodButton.astro";
import BlaButton from "../components/BlaButton.astro";
---

<MainLayout title="FORSIDE">
	<article>
