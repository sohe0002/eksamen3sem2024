import kaffeflyver from "../assets/images/kaffeflyver.webp";
import steps from "../assets/images/steps.png";
import ansatte from "../assets/images/ansatte.webp";
import farm from "../assets/images/farm.webp";
import farmer2 from "../assets/images/farmer2.webp";
import farmer3 from "../assets/images/farmer3.webp";
import farmer4 from "../assets/images/farmer4.webp";
